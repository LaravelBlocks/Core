{
    "name": "laravelblocks/core",
    "description": "Extensible content block system for Laravel.",
    "license": "MIT",
    "keywords": ["laravel"],
    "authors": [
        {
            "name": "Nathan Heffley",
            "email": "nathan@nathanheffley.com",
            "homepage": "https://www.nathanheffley.com",
            "role": "Developer"
        }
